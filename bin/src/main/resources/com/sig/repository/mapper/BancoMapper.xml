<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sig.repository.mapper.BancoMapper">
    

    <select id="getAll" resultType="Banco">
        SELECT id
        	,nombre
        	,estado
        	,fecha_registro
        	,usuario
        	,terminal
         FROM "banco" 
         WHERE estado != 'E'
    </select>
    
  
    
</mapper>